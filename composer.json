{
    "name": "anwar/smaregi",
    "type": "library",
    "license": "MIT",
    "description": "Smaregi app api implementation for laravel",
    "keywords": [
        "anwar",
        "laravel",
        "smaregi"
    ],
    "authors": [
        {
            "name": "Anwar Jahid",
